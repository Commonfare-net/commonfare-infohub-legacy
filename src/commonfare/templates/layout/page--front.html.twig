
<!-- Header and Navbar -->
<header class="main-header">
  <nav class="navbar navbar-default" role="navigation">
    <div class="container">
      <div class="row">
        <div class="navbar-header col-md-3">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navigation">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          {% if page.header %}
            {{ page.header }}
          {% endif %}
        </div>

        <!-- Navigation -->
        <div class="col-md-9 col-xs-9 primary-menu-container">
          {% if page.primary_menu %}
            {{ page.primary_menu }}
          {% endif %}
        </div>
        <!--End Navigation -->

        <!-- Mobile only sandwich -->
        <div class="col-md-3 col-xs-3 mobile-menu-container text-right">
          <div class="mobile-menu large text-white">
            <i class="fa fa-bars" aria-hidden="true"></i>
          </div>
        </div>
        <!--End Mobile only sandwich -->

      </div>
    </div>
  </nav>

  <!-- Banner -->
  {% if is_front and page.welcome_banner_text %}
    <div class="container welcome-banner-text">
      <div class="row">
        <div class="col-md-12">
          {{ page.welcome_banner_text }}
        </div>
      </div>
    </div>
  {% endif %}
  <!-- End Banner -->

  <!-- Mobile menu panel  -->
  <div class="mobile-menu-item ">
    <div class="close-button text-right">
      <i class="text-red fa fa-times" aria-hidden="true"></i>
    </div>
    <div class="language-selector text-center medium">
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <span class="label">Language</span> <span class="curr"></span> <span class="caret"></span>
        </button>
        <ul class="dropdown-menu">
          <li><a href="#it" data-lang="it">IT - Italiano</a></li>
          <li><a href="#hr" data-lang="hr">HR - Hrvatski</a></li>
          <li><a href="#nl" data-lang="nl">NL - Nederlands</a></li>
          <li><a href="#en" data-lang="en">EN - English</a></li>
        </ul>
      </div>
    </div>
    <div class="logo-red">
      <a href="/#about"><img src="/themes/commonfare/logo_red.svg" alt="About"></a>
    </div>
  </div>
  <!-- End Mobile menu panel  -->

</header>
<!--End Header & Navbar -->



